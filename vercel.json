{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "api/*.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/dash_app",
        "dest": "/api/dash_app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  